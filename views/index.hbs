									</p>
								</div>
							</div>
							<h4><a href="/admin/furniture/{{id}}">{{name}}</a></h4>
							<p class="arrival-product-price">$ {{cost}}</p>
						</div>
					</div>
					{{/each}}
					{{else}}
					<p> furnitures is a empty</p>
					{{/if}}
				</div>
			</div>
		</div>
